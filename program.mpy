def map(map_func, list):
    new_list = []
    index = 0
    while index < list.__len__():
        element = list.get(index)
        new_list.append(map_func(element))
        index = index + 1
    #end
    return new_list
#end

def map_fun(a):
    return a * a
#end

a = [1, 2, 3]
b = map(map_fun, a)
print(b)


